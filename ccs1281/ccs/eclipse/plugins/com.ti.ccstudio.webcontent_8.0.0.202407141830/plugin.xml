   <extension
         point="org.eclipse.core.runtime.preferences">
         <initializer
               class="com.ti.ccstudio.webcontent.internal.PreferenceInitializer">
         </initializer>
   </extension>
   <extension
         point="com.ti.common.ui.preferences">
      <preferences
            class="com.ti.ccstudio.webcontent.internal.PreferencesContribution">
      </preferences>
   </extension>
-->
</plugin>
