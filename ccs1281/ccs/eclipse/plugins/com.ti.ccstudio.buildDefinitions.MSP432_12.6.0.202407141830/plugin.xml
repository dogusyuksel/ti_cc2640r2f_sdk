<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<plugin>

	<extension point="com.ti.ccstudio.project.core.buildDefinitionContributions">
		<xmlRepository 
			isaFamily="MSP432" 
			location="resources/buildDefinitions"/>
	</extension>
	
	<extension point="com.ti.ccstudio.project.core.deviceDefinitionContributions">
		<xmlRepository 
			isaFamily="MSP432" 
			location="resources/deviceDefinitions"/>
		<device class="com.ti.ccstudio.deviceModel.MSP432.CustomMSP432Device"/>
		<realDeviceFactory class="com.ti.ccstudio.deviceModel.MSP432.MSP432DeviceFactory"/>
	</extension>
	
	<extension point="com.ti.common.project.core.configurationInitializerDelegates">
		<delegate 
			class="com.ti.ccstudio.buildDefinitions.MSP432.MSP432ConfigurationInitializerDelegate">
			<projectType projectTypeID="com.ti.ccstudio.buildDefinitions.MSP432.ProjectType"/>
		</delegate>
	</extension>
	
</plugin>
