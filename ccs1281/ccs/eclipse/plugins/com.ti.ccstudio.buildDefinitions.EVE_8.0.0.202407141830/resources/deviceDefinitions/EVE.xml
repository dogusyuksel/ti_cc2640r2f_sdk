<?xml version="1.0" encoding="UTF-8"?>

<deviceDefinitions>

	<isaFamily
		id="EVE"
		name="EVE"
		buildToolSuffix="-arp32"
		binaryParser="org.eclipse.cdt.core.GNU_ELF"
		supportedEndiannessTypes="little"
		supportedOutputFormats="elf:eabi"/>
		
	<isa
		id="EVE"
		name="EVE"
		family="EVE"
		ssId="ARP32"/>


	<!-- Generic devices -->
	
	<genericDevice
		id="EVE.genericDevice.EVE"
		name="Generic EVE Device"
		deviceVariant="EVE"
		isa="EVE">
		<property name="IsElfDefault" value="true"/>
		<property name="RTSlib" value="rtsarp32_v200.lib"
			whenEndianness="little"
			whenToolChain="TI"
			whenOutputFormat="ELF"/>
		<property name="CompilerBuildOptions" value="--silicon_version=210 -I&quot;${CG_TOOL_ROOT}/include/vcop&quot;"
			whenToolChain="TI"/>
	</genericDevice>


	<!-- Real devices -->
	
	<realDeviceFactory
		deviceVariant="EVE"
		isa="EVE">
		<property name="IsElfDefault" value="true"/>
		<property name="CompilerBuildOptions" value="--silicon_version=210 -I&quot;${CG_TOOL_ROOT}/include/vcop&quot;"
			whenToolChain="TI"/>
	</realDeviceFactory>
		

</deviceDefinitions>
